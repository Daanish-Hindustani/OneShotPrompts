# Core
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Auth (NextAuth)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=replace_me
GOOGLE_CLIENT_ID=replace_me
GOOGLE_CLIENT_SECRET=replace_me

# Database
# Connect to Supabase via connection pooling
DATABASE_URL=postgresql://postgres.<project_ref>:[YOUR-PASSWORD]@aws-0-us-west-2.pooler.supabase.com:6543/postgres?pgbouncer=true
# Direct connection to the database. Used for migrations
DIRECT_URL=postgresql://postgres.<project_ref>:[YOUR-PASSWORD]@aws-0-us-west-2.pooler.supabase.com:5432/postgres

# Billing (Stripe)
STRIPE_SECRET_KEY=replace_me
STRIPE_WEBHOOK_SECRET=replace_me
STRIPE_PRICE_BASIC=price_basic_id
STRIPE_PRICE_PRO=price_pro_id
STRIPE_PRICE_TEAM=price_team_id

# Dev-only subscription bypass (non-production only)
SUBSCRIPTION_BYPASS=false
SUBSCRIPTION_BYPASS_TIER=BASIC

# OpenAI
OPENAI_API_KEY=replace_me

# Rate limiting (Upstash)
UPSTASH_REDIS_REST_URL=replace_me
UPSTASH_REDIS_REST_TOKEN=replace_me
